{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"styles": {
		"blocks": {
			"core/quote":{
				"variations": {
					"hand-drawn-red":{
                        "color": {
                            "text": "red"
                        },
						"border": {
							"color": "red",
							"style": "solid",
							"width": "2px",
							"radius": "255px 15px 225px 15px/15px 225px 15px 255px"
						},
						"shadow": "var(--wp--preset--shadow--natural)"
					},
					"hand-drawn-purple":{
                        "color": {
                            "text": "purple"
                        },
						"border": {
							"color": "purple",
							"style": "solid",
							"width": "2px",
							"radius": "255px 15px 225px 15px/15px 225px 15px 255px"
						},
						"shadow": "var(--wp--preset--shadow--natural)"
					}
				}
			}
		}
	}
}

